{
  "name": "graphql-mysql-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/app.ts",
    "start": "node build/app.js",
    "migrate:make": "knex --knexfile ./src/db/knexfile.ts migrate:make",
    "migrate:latest": "knex --knexfile ./src/db/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./src/db/knexfile.ts migrate:rollback",
    "prod:migrate:make": "knex --knexfile ./build/db/knexfile.js migrate:make",
    "prod:migrate:latest": "knex --knexfile ./build/db/knexfile.js migrate:latest",
    "prod:migrate:rollback": "knex --knexfile ./build/db/knexfile.js migrate:rollback"
  },
  "keywords": [],
  "author": "Socheat CHEA",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.8.7",
    "dotenv": "^16.3.1",
    "knex": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
